{
  "name": "v-google-translate",
  "version": "0.1.5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i7eo/v-google-translate.git"
  },
  "keywords": [
    "Vue2.x",
    "Vue",
    "google translate",
    "google",
    "translate",
    "vue google translate"
  ],
  "files": [
    "lib",
    "public",
    "src",
    "README.md",
    "README_zh.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name v-google-translate src/packages/index.js",
    "test:unit": "vue-cli-service test:unit",
    "coverage": "open ./coverage/lcov-report/index.html",
    "lint": "vue-cli-service lint"
  },
  "main": "./lib/v-google-translate.common.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/**/**/*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "author": "i7eo <george704t@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/i7eo/v-google-translate/issues"
  },
  "homepage": "https://github.com/i7eo/v-google-translate#readme"
}
